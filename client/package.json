{
  "name": "tellgpt",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "browser:build": "ionic build",
    "browser:build:prod": "ionic build && rm -rf ../server/public/client && cp -r ./www ../server/public/client",
    "android:add": "cap add android",
    "android:sync": "cap sync",
    "android:copy": "cap copy android",
    "android:keystore:create": "keytool -genkey -v -keystore easyNFT.keystore -alias easyNFT -keyalg RSA -keysize 2048 -validity 10000",
    "android:build": "NODE_ENV=production ionic capacitor build android --prod && npm run android:sync",
    "android:apk:debug": "npm run android:build && cd ./android && ./gradlew assembleDebug",
    "android:apk:debug:win": "npm run android:copy & cd android & ./gradlew assembleDebug",
    "android:apk:prod": "npm run android:build && npm run android:apk:sign",
    "android:apk:sign": "apksigner sign --ks easyNFT.keystore --out ./android/app/build/outputs/apk/release/app-release.apk ./android/app/build/outputs/apk/release/app-release-unsigned.apk",
    "android:bundle:prod": "npm run android:build && cd ./apps/easynft/android && ./gradlew bundleRelease && cd ../../../ && npm run android:bundle:sign",
    "android:bundle:sign": "jarsigner -storepass halo33221 -sigalg SHA1withRSA -digestalg SHA1 -keystore ./apps/easynft/easyNFT.keystore -signedjar ./apps/easynft/android/app/build/outputs/bundle/release/app-release.aab ./apps/easynft/android/app/build/outputs/bundle/release/app-release.aab nft-party",
    "ios:add": "nx run easynft:add:ios",
    "ios:sync": "nx run easynft:sync:ios",
    "ios:copy": "nx run easynft:copy:ios",
    "ios:build": "NODE_ENV=production nx run easynft:build:ios --prod && npm run android:copy",
    "analyze": "webpack-bundle-analyzer ./dist/apps/easynft/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@capacitor-community/speech-recognition": "^3.0.0-0",
    "@capacitor-community/text-to-speech": "^2.0.1",
    "@capacitor/android": "4.6.2",
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "4.6.2",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/keyboard": "4.1.1",
    "@capacitor/status-bar": "4.1.1",
    "@ionic/angular": "^6.5.0",
    "ionicons": "^6.0.3",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@capacitor/cli": "4.6.2",
    "@ionic/angular-toolkit": "^6.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ts-node": "~8.3.0",
    "typescript": "^4.9.4"
  },
  "description": "An Ionic project"
}
